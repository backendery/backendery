{
  "name": "backendery",
  "version": "0.3.4",
  "private": true,
  "description": "A small React.js single-page app that serves as a homepage for the Backendery digital studio",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/backendery/backendery.git"
  },
  "license": "BUSL-1.1",
  "maintainers": [
    {
      "name": "Backendery",
      "email": "hi@backendery.digital",
      "url": "https://github.com/backendery"
    }
  ],
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation vite build --config=./vite.config.ts --mode=production",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation vite --config=./vite.config.ts --clearScreen=false --host --mode=development",
    "lint-es": "cross-env NODE_OPTIONS=--no-deprecation eslint \"**/*.{js,jsx,ts,tsx}\" --config=./eslint.config.mjs --cache",
    "lint-es:fix": "cross-env NODE_OPTIONS=--no-deprecation eslint \"**/*.{js,jsx,ts,tsx}\" --config=./eslint.config.mjs --fix --cache",
    "lint-prettier": "cross-env NODE_OPTIONS=--no-deprecation prettier \"**/*.{js,jsx,ts,tsx}\" --config=./prettier.config.mjs --check --cache",
    "lint-prettier:fix": "cross-env NODE_OPTIONS=--no-deprecation prettier \"**/*.{js,jsx,ts,tsx}\" --config=./prettier.config.mjs --write --cache",
    "lint-style": "cross-env NODE_OPTIONS=--no-deprecation stylelint \"**/*.{css,scss}\" --config=./stylelint.config.mjs --cache --max-warnings=0",
    "lint-style:fix": "cross-env NODE_OPTIONS=--no-deprecation stylelint \"**/*.{css,scss}\" --config=./stylelint.config.mjs --fix",
    "prepare": "husky install",
    "preview": "cross-env NODE_OPTIONS=--no-deprecation vite preview --config=./vite.config.ts",
    "types": "cross-env NODE_OPTIONS=--no-deprecation tsc --noEmit --pretty"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "pnpm lint-es:fix"
    ],
    "*.{css,scss}": [
      "pnpm lint-style:fix"
    ],
    "*.{json,md,html,yml,yaml}": [
      "pnpm lint-prettier:fix"
    ]
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "dependencies": {
    "@sentry/react": "~10.30.0",
    "axios": "~1.7.9",
    "axios-retry": "~4.5.0",
    "formik": "~2.4.9",
    "overlayscrollbars": "~2.10.1",
    "react": "~19.2.3",
    "react-dom": "~19.2.3",
    "react-error-boundary": "~6.0.0",
    "react-fast-marquee": "~1.6.5",
    "react-responsive": "~10.0.1",
    "react-router": "~7.10.1",
    "react-router-dom": "~7.10.1",
    "react-typed": "~2.0.12",
    "react-use": "~17.6.0",
    "sanitize.css": "~13.0.0",
    "use-scramble": "~2.2.15",
    "yup": "~1.7.1",
    "zod": "~4.1.12"
  },
  "devDependencies": {
    "@svgr/plugin-jsx": "~8.1.0",
    "@svgr/plugin-svgo": "~8.1.0",
    "@swc/core": "~1.15.1",
    "@swc/types": "~0.1.25",
    "@types/node": "~25.0.2",
    "@types/react": "~19.2.7",
    "@types/react-dom": "~19.2.3",
    "@types/react-router": "~5.1.20",
    "@types/react-router-dom": "~5.3.3",
    "@vitejs/plugin-react-swc": "~4.2.2",
    "autoprefixer": "~10.4.22",
    "browserslist": "~4.24.4",
    "cross-env": "~10.1.0",
    "dotenv": "~17.2.3",
    "eslint": "~9.39.1",
    "eslint-config-canonical": "~47.3.7",
    "eslint-plugin-simple-import-sort": "~12.1.1",
    "globals": "~16.5.0",
    "husky": "~9.1.7",
    "lightningcss": "~1.30.2",
    "lint-staged": "~16.2.6",
    "postcss": "~8.5.6",
    "postcss-html": "~1.8.0",
    "postcss-import": "~16.1.0",
    "postcss-jsx": "~0.36.4",
    "postcss-normalize": "~13.0.1",
    "postcss-preset-env": "~10.1.5",
    "postcss-scss": "~4.0.9",
    "postcss-syntax": "~0.36.2",
    "prettier": "~3.5.3",
    "sass": "~1.96.0",
    "size-limit": "~11.2.0",
    "sort-package-json": "~2.12.0",
    "stylelint": "~16.12.0",
    "stylelint-config-recommended-scss": "~14.1.0",
    "stylelint-config-standard": "~36.0.1",
    "stylelint-scss": "~6.11.1",
    "terser": "~5.39.0",
    "ts-essentials": "~10.1.1",
    "ts-node": "~10.9.2",
    "tsx": "~4.20.6",
    "typescript": "~5.9.3",
    "typescript-eslint": "~8.49.0",
    "vite": "~6.4.1",
    "vite-plugin-svgr": "~4.3.0",
    "vite-tsconfig-paths": "~5.1.4"
  },
  "packageManager": "pnpm@9.15.4",
  "engines": {
    "node": "~18.20.2 || >=20.19.1",
    "pnpm": "~9.15.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core"
    ],
    "overrides": {},
    "ignoredBuiltDependencies": []
  },
  "size-limit": [
    {
      "limit": "50 kB",
      "path": "dist/**/*.js"
    },
    {
      "limit": "100 kB",
      "path": "dist/**/*.css"
    }
  ],
  "trustedDependencies": [
    "@swc/core"
  ]
}